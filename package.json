    "commander": "^11.0.0",
    "esm": "^3.2.25",
    "figlet": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
